include "std:util";

set map = fun (arr, f) {
    set res = [];
    set iter = fun (array) {
        if (length(array) == 0) {
            return;
        } else {
            put(array)
            anew res = push(res, f(array[0]));
            iter(tail(array));
        }
    };
    iter(arr)
    return res;
};
